void main() {
  List<int> numbers = [12, 4, 9, 6, 15];
  String sortedNumbers = sortNumbers(numbers);
  print('Отсортированные числа: $sortedNumbers');
}

String sortNumbers(List<int> numbers) {
  List<int> sortedList = new List.from(numbers); 
  sortedList.sort(); 

  return sortedList.join(', '); 
}
